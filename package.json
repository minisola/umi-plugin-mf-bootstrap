    ]
  },
  "devDependencies": {
    "@umijs/test": "^3.0.5",
    "father-build": "^1.17.2",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.1",
    "yorkie": "^2.0.0",
    "@umijs/types": "^3.0.0",
    "cross-env": "^6.0.3",
    "np": "^5.0.3",
    "umi": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/minisola/umi-plugin-mf-bootstrap.git"
  }
